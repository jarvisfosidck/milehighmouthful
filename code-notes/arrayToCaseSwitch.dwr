<?xml version="1.0"?><dwquery>  <queryparams matchcase="false" ignorewhitespace="false" useregexp="true" wholeword="false" />  <find searchmode="document">    <qtext qname=",([a-zA-Z]+)" qraw="true"></qtext>  </find>  <replace action="replaceText" param1="case &quot;$1&quot; : \n\t\t\t $1(evt); \n\t\t\t break;\n" param2=""/></dwquery>